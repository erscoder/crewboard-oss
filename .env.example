# Supabase database (connection pooler for Cloudflare Pages)
DATABASE_URL=postgresql://postgres:PASSWORD@PROJECT_ID.supabase.co:6543/postgres?pgbouncer=true&connection_limit=1&pool_timeout=30
# Direct connection for migrations / Prisma CLI
DIRECT_DATABASE_URL=postgresql://postgres:PASSWORD@PROJECT_ID.supabase.co:5432/postgres

# Supabase API
SUPABASE_URL=https://PROJECT_ID.supabase.co
SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY

# Local paths / ports
CLAWDBOT_ROOT=/Users/kike/clawd
PORT=3010
